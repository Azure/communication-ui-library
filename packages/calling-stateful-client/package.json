{
  "name": "@internal/calling-stateful-client",
  "version": "1.0.0",
  "description": "Stateful proxy over the Azure Communication Services Calling JavaScript SDK",
  "module": "dist/dist-esm/index.js",
  "main": "dist/dist-cjs/index.js",
  "types": "dist/calling-stateful-client.d.ts",
  "scripts": {
    "build": "rushx by-flavor \"npm run build:by-flavor\"",
    "build:esm": "if-env FLAVOR=stable && rushx preprocess && tsc -project tsconfig.preprocess.json || (if-env FLAVOR=beta && tsc)",
    "build:cjs": "rollup -c --silent --failAfterWarnings",
    "build:watch": "rushx build",
    "clean": "rimraf dist && rimraf preprocessed",
    "current-flavor": "echo You are running under FLAVOR: && env-cmd -f ../../common/config/env/.env node -p process.env.FLAVOR",
    "by-flavor": "rushx current-flavor && env-cmd -f ../../common/config/env/.env",
    "preprocess": "cpx ./src/** ./preprocessed && babel ./src --out-dir ../preprocessed --extensions \".ts,.tsx\" --keep-file-extension --config-file ./.babelrc.js --relative && rimraf ../preprocessed",
    "build:by-flavor": "rushx clean && npm run build:esm && rushx build:cjs && api-extractor run --local",
    "test:by-flavor": "(if-env FLAVOR=stable && rushx preprocess || if-env FLAVOR=beta && echo \"skip preprocess\") && jest",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "test": "rushx by-flavor \"npm run test:by-flavor\"",
    "test:coverage": "rushx by-flavor \"npm run test:by-flavor -- --coverage\"",
    "lint": "eslint \"*/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "api-extractor": "rushx by-flavor \"npm run build:esm\" && api-extractor run --local",
    "generate-doc": "api-documenter markdown -i temp -o docGen"
  },
  "dependencies": {
    "@azure/communication-calling": "1.3.2-beta.1 || 1.3.2",
    "@azure/communication-common": "1.0.0",
    "@internal/acs-ui-common": "1.0.0",
    "events": "~3.3.0",
    "immer": "9.0.6"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/cli": "~7.16.0",
    "@azure/core-auth": "1.3.2",
    "@microsoft/api-documenter": "~7.12.11",
    "@microsoft/api-extractor": "~7.18.0",
    "@types/jest": "~26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@zerollup/ts-transform-paths": "~1.7.18",
    "copyfiles": "^2.4.1",
    "cpx": "~1.5.0",
    "cross-env": "~7.0.3",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsdoc": "~36.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint": "^7.7.0",
    "jest": "26.6.0",
    "rimraf": "^2.6.2",
    "rollup": "~2.42.4",
    "prettier": "2.3.1",
    "ts-jest": "^26.4.4",
    "typescript": "4.3.5",
    "@types/react": "^16.9.49",
    "env-cmd": "~10.1.0",
    "if-env": "~1.0.4"
  }
}
