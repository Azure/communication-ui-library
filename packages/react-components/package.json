{
  "name": "@internal/react-components",
  "version": "1.0.0-beta.8",
  "description": "React component library",
  "module": "dist/dist-esm/index.js",
  "main": "dist/dist-cjs/index.js",
  "types": "dist/react-components.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rushx clean && rushx build:esm && rushx build:cjs && api-extractor run --local",
    "build:cjs": "rollup -c --silent --failAfterWarnings",
    "build:esm": "tsc",
    "build:watch": "rushx build",
    "clean": "rimraf dist",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "snapshot:update": "npm run test -- --update-snapshot",
    "copy-original": "cpx ./src/** ./preprocessed",
    "preprocess": "rushx copy-original && babel ./src --out-dir ../preprocessed --extensions \".ts,.tsx\" --keep-file-extension --config-file ./.babelrc.js --relative && rimraf ../preprocessed",
    "preprocess:stable": "cross-env FLAVOR=stable rushx preprocess",
    "clean:preprocess": "rimraf ./preprocessed",
    "build:stable": "rushx clean:preprocess && rushx preprocess:stable && tsc -project tsconfig.preprocess.json && rushx build:cjs && api-extractor run --local",
    "test:stable": "rushx clean:preprocess && rushx preprocess:stable && jest --verbose ./preprocessed -c jest.config.preprocess.js",
    "api-extractor": "tsc && api-extractor run --local",
    "generate-doc": "api-documenter markdown -i temp -o docGen",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "lint": "eslint \"*/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet"
  },
  "dependencies": {
    "@fluentui/react": "~8.17.2",
    "@fluentui/react-icons": "~1.1.137",
    "@fluentui/react-northstar": "^0.57.0",
    "@uifabric/react-hooks": "~7.13.11",
    "copy-to-clipboard": "~3.3.1",
    "@internal/acs-ui-common": "1.0.0-beta.8",
    "react-aria-live": "^2.0.5",
    "react-linkify": "^1.0.0-alpha",
    "html-to-react": "~1.4.5",
    "uuid": "^8.1.0"
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0 <18.0.0",
    "@types/react-dom": ">=16.8.0 <18.0.0",
    "react": ">=16.8.0 <18.0.0",
    "react-dom": ">=16.8.0 <18.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/cli": "~7.16.0",
    "@babel/preset-env": "7.13.10",
    "@mdx-js/react": "^1.6.22",
    "@microsoft/api-documenter": "~7.12.11",
    "@microsoft/api-extractor": "~7.18.0",
    "@rollup/plugin-json": "~4.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/enzyme": "~3.10.8",
    "@types/jest": "~26.0.22",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/node": "^14.14.10",
    "@types/react-aria-live": "^2.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-linkify": "^1.0.0",
    "@types/react": "^16.9.49",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@zerollup/ts-transform-paths": "~1.7.18",
    "ajv": "^6.9.1",
    "babel-jest": "^26.6.0",
    "babel-loader": "8.1.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.8.3",
    "cpx": "~1.5.0",
    "cross-env": "~7.0.3",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.6",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsdoc": "~36.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint": "^7.7.0",
    "husky": "^4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.0.0",
    "jest": "26.6.0",
    "json-stringify-safe": "^5.0.1",
    "nan": "^2.14.1",
    "node-forge": "0.10.0",
    "prettier": "2.3.1",
    "pretty-quick": "^3.1.0",
    "react-dom": "16.13.1",
    "react-is": "~17.0.1",
    "react-test-renderer": "^16.14.0",
    "react": "~16.14.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^2.6.2",
    "rollup": "~2.42.4",
    "source-map-explorer": "^2.5.0",
    "styled-components": "~5.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "type-fest": "~2.5.4",
    "typescript": "4.3.5",
    "webpack": "5.38.1"
  }
}
