    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint": "^7.7.0",
    "husky": "^4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.0.0",
    "jest": "26.6.0",
    "json-stringify-safe": "^5.0.1",
    "nan": "^2.14.1",
    "node-forge": "0.10.0",
    "prettier": "2.0.5",
