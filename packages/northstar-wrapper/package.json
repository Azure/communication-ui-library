{
  "name": "@internal/northstar-wrapper",
  "version": "1.5.1-beta.4",
  "private": "true",
  "description": "Package to wrap the Fluent NorthStar UI library, for short-term React 18 compatibility",
  "scripts": {
    "build": "tsc",
    "build:watch": "echo skip",
    "build:stable": "tsc",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "test": "echo skip",
    "test:stable": "echo skip",
    "test:coverage": "echo skip",
    "lint": "eslint --max-warnings 0 \"**/*.{js,ts,tsx}\"",
    "lint:fix": "rushx lint -- --fix",
    "lint:quiet": "rushx lint -- --quiet"
  },
  "dependencies": {
    "@fluentui/react-northstar": "0.61.0",
    "@fluentui/react": "~8.98.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "~2.22.1",
    "prettier": "2.3.1",
    "react": "~16.14.0",
    "react-dom": "16.13.1",
    "scheduler": "^0.20.0",
    "rimraf": "^2.6.2",
    "typescript": "3.8.3"
  }
}