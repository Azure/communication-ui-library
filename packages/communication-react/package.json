    "@microsoft/api-extractor": "~7.23.2",
    "@rollup/plugin-json": "~4.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/dompurify": "^3.0.2",
    "@types/jest": "~29.5.2",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/node": "^20.4.5",
