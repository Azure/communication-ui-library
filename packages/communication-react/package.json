  },
  "main": "./dist/dist-cjs/communication-react/index.js",
  "module": "./dist/dist-esm/communication-react/src/index.js",
  "scripts": {
    "copy-original": "cpx ../*/src/** ./preprocess-dist & renamer --find src --replace preprocessed ./preprocess-dist/**",
    "preprocess": "babel ../*/src --out-dir ../preprocessed --extensions \".ts,.tsx\" --keep-file-extension --config-file ./.babelrc.js --relative",
    "preprocess:public": "cross-env FLAVOR=public rushx preprocess",
    "preprocess:beta": "cross-env FLAVOR=beta rushx preprocess",
    "copy-preprocess": "cpx ../*/preprocessed/** ./preprocess-dist & renamer --find preprocessed --replace src ./preprocess-dist/**",
    "clean:preprocess": "rimraf ../**/preprocessed && rimraf ../**/preprocess-dist",
    "build": "rushx clean && rushx check-deps && rushx build:esm && rushx build:cjs && api-extractor run --local",
    "build:cjs": "rollup -c --silent",
    "build:esm": "ttsc",
    "build:public": "rushx copy-original && rushx preprocess:public && rushx copy-preprocess && ttsc -project tsconfig.preprocess.json && rushx build:cjs && api-extractor run --local && rushx clean:preprocess",
    "build:beta": "rushx copy-original && rushx preprocess:beta && rushx copy-preprocess && ttsc -project tsconfig.preprocess.json && rushx build:cjs && api-extractor run --local && rushx clean:preprocess",
    "build:watch": "",
    "clean": "rimraf dist",
    "test": "",
    "test:coverage": "",
    "check-deps": "node scripts/verifyDependencies",
    "api-extractor": "tsc && api-extractor run --local",
    "generate-doc": "api-documenter markdown -i temp -o docGen",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "lint": "eslint \"*/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "postpack": "copyfiles -E \"./*.tgz\" release"
  },
  "devDependencies": {
    "@azure/core-auth": "1.3.2",
    "@babel/core": "~7.16.0",
    "@babel/cli": "~7.16.0",
    "@babel/plugin-syntax-typescript": "~7.16.0",
    "babel-plugin-conditional-compilation": "~0.0.1",
    "@internal/calling-component-bindings": "1.0.0-beta.7",
