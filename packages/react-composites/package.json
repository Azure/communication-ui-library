{
  "name": "react-composites",
  "version": "1.0.0-beta.0",
  "description": "Complete Azure Communication Services experiences",
  "module": "dist/dist-esm/index.js",
  "main": "dist/dist-cjs/index.js",
  "types": "dist/dist-esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rushx build:esm && rushx build:cjs && api-extractor run --local",
    "build:cjs": "rollup -c --silent --failAfterWarnings",
    "build:esm": "tsc && copyfiles -u 1 src/**/assets/**/* dist/dist-esm",
    "build:watch": "rushx build",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "snapshot:update": "npm run test -- --update-snapshot",
    "api-extractor": "tsc && api-extractor run --local",
    "generate-doc": "api-documenter markdown -i temp -o docGen",
    "prettier": "prettier --write --config ../../.prettierrc \"**/*.js\" \"**/*.ts\"",
    "prettier:check": "prettier --check --config ../../.prettierrc \"**/*.js\" \"**/*.ts\"",
    "lint": "eslint \"*/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "postpack": "copyfiles -E \"./*.tgz\" private-preview"
  },
  "dependencies": {
    "acs-ui-common": "~1.0.0-beta.0",
    "calling-stateful-client": "~1.0.0-beta.0",
    "@azure/acs-calling-selector": "~1.0.0-beta.0",
    "@azure/communication-calling": "1.0.1-beta.1",
    "@azure/communication-chat": "1.0.0",
    "@azure/communication-common": "1.0.0",
    "@azure/communication-signaling": "1.0.0-beta.5",
    "@azure/core-http": "^1.2.3",
    "@fluentui/react": "^7.117.1",
    "@fluentui/react-icons": "^0.3.9",
    "@fluentui/react-icons-northstar": "^0.51.2",
    "@fluentui/react-northstar": "^0.51.2",
    "@fluentui/react-theme-provider": "^0.18.0",
    "@uifabric/react-hooks": "~7.13.11",
    "copy-to-clipboard": "~3.3.1",
    "chat-stateful-client": "~1.0.0-beta.0",
    "@azure/acs-chat-selector": "~1.0.0-beta.0",
    "react-aria-live": "^2.0.5",
    "react-linkify": "^1.0.0-alpha",
    "memoize-one": "~5.2.1",
    "events": "~3.3.0",
    "react-components": "1.0.0-beta.0",
    "uuid": "^8.1.0",
    "reselect": "~4.0.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@mdx-js/react": "^1.6.22",
    "@microsoft/api-documenter": "~7.12.11",
    "@microsoft/api-extractor": "~7.13.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "^26.0.20",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/node": "^14.14.10",
    "@types/react-aria-live": "^2.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-linkify": "^1.0.0",
    "@types/react": "^16.9.49",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ajv": "^6.9.1",
    "babel-jest": "^26.6.0",
    "babel-loader": "8.1.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.8.3",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint": "^7.7.0",
    "husky": "^4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.0.0",
    "jest": "26.6.0",
    "json-stringify-safe": "^5.0.1",
    "nan": "^2.14.1",
    "node-forge": "0.10.0",
    "prettier": "2.0.5",
    "pretty-quick": "^3.1.0",
    "react-dom": "^16.13.1",
    "react-is": "~17.0.1",
    "react-test-renderer": "^16.14.0",
    "react": "^16.13.1",
