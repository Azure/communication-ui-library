    "typescript": "4.1.5",
    "uuid": "^8.1.0",
    "webpack": "4.44.2"
  },
  "beachball": {
    "tag": "next",
    "disallowedChangeTypes": [
      "major",
      "minor",
      "patch"
    ]
  }
}
