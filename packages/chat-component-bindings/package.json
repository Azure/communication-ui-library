{
  "name": "@internal/chat-component-bindings",
  "version": "1.0.0-beta.8",
  "description": "Component bindings specific to components that integrate with the Azure Communication Services Chat JavaScript SDK",
  "module": "dist/dist-esm/index.js",
  "main": "dist/dist-cjs/index.js",
  "types": "dist/chat-component-bindings.d.ts",
  "scripts": {
    "build": "rushx clean && rushx build:esm && rushx build:cjs && api-extractor run --local",
    "build:esm": "tsc",
    "build:cjs": "rollup -c --silent --failAfterWarnings",
    "build:watch": "rushx build",
    "clean": "rimraf dist",
    "copy-original": "cpx ./src/** ./preprocessed",
    "preprocess": "rushx copy-original && babel ./src --out-dir ../preprocessed --extensions \".ts,.tsx\" --keep-file-extension --config-file ./.babelrc.js --relative && rimraf ../preprocessed",
    "preprocess:stable": "cross-env FLAVOR=stable rushx preprocess",
    "clean:preprocess": "rimraf ./preprocessed",
    "build:stable": "rushx clean:preprocess && rushx preprocess:stable && tsc -project tsconfig.preprocess.json && rushx build:cjs && api-extractor run --local",
    "test:stable": "rushx clean:preprocess && rushx preprocess:stable && jest --verbose ./preprocessed -c jest.config.preprocess.js",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint \"*/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "api-extractor": "tsc && api-extractor run --local",
    "generate-doc": "api-documenter markdown -i temp -o docGen"
  },
  "dependencies": {
    "@azure/communication-chat": "1.1.0",
    "@azure/communication-common": "1.0.0",
    "@azure/communication-signaling": "1.0.0-beta.5",
    "@azure/core-paging": "~1.1.3",
    "@internal/acs-ui-common": "1.0.0-beta.8",
    "@internal/chat-stateful-client": "1.0.0-beta.8",
    "@internal/react-components": "1.0.0-beta.8",
    "reselect": "~4.0.0",
    "memoize-one": "~5.2.1"
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0 <18.0.0",
    "react": ">=16.8.0 <18.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/cli": "~7.16.0",
    "@microsoft/api-documenter": "~7.12.11",
    "@microsoft/api-extractor": "~7.18.0",
    "@types/react": "^16.9.49",
    "@zerollup/ts-transform-paths": "~1.7.18",
    "copyfiles": "^2.4.1",
    "cpx": "~1.5.0",
    "cross-env": "~7.0.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsdoc": "~36.1.0",
    "prettier": "2.3.1",
    "react": "~16.14.0",
    "rimraf": "^2.6.2",
    "rollup": "~2.42.4",
    "typescript": "4.3.5",
    "jest": "26.6.0",
    "@types/jest": "~26.0.22"
  }
}
