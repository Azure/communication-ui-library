{
  "name": "acs-calling-sample",
  "version": "0.0.1",
  "private": true,
  "description": "This is a sample application to show how the ACS Calling Web SDK can be used to build a group calling experience. The client-side application is a React based user interface which uses React Hooks for handling complex state while leveraging Microsoft Fluent UI for how things look.",
  "scripts": {
    "build": "cd ../Server && npm run build && cd ../Calling && npm run build:app",
    "build:app": "react-scripts build",
    "build:prod": "npm run build && copyfiles -u 3 ../Server/dist/**/* dist && copyfiles ./build/**/* dist",
    "start": "concurrently \"cd ../Server && npm run start:dev\" \"npm run start:app\"",
    "start:app": "rimraf ./build && react-scripts start",
    "start:debug": "concurrently \"cd ../Server && npm run start:debug\" \"npm run start\"",
    "start:prod": "node dist/server.js",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "snapshot:update": "npm run test -- --update-snapshot",
    "prettier": "prettier --write --config .prettierrc **/*.js **/*.ts",
    "prettier:check": "prettier --check --config .prettierrc **/*.js **/*.ts",
    "lint": "eslint */**/*.{ts,tsx}",
    "lint:fix": "eslint */**/*.{ts,tsx} --fix",
    "lint:quiet": "eslint */**/*.{ts,tsx} --quiet",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!/node_modules/",
      "!src/mocks/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@azure/communication-administration": "^1.0.0-beta.3",
    "@azure/communication-calling": "1.0.0-beta.2",
    "@azure/communication-common": "1.0.0-beta.3",
    "@azure/communication-ui": "^0.1.0",
    "@babel/preset-react": "^7.12.7",
    "@fluentui/react": "^7.117.1",
    "@fluentui/react-icons-northstar": "^0.51.0",
    "@fluentui/react-northstar": "^0.51.0",
    "@fluentui/react-theme-provider": "^0.18.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.5.1",
    "merge": "^1.2.1",
    "oidc-client": "^1.9.0",
    "preval.macro": "^5.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.3",
    "reactstrap": "^8.4.1",
    "uuid": "^8.1.0",
    "@azure/core-http": "~1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@storybook/addon-actions": "^6.1.1",
    "@storybook/addon-essentials": "^6.1.1",
    "@storybook/addon-knobs": "^6.1.9",
    "@storybook/addon-links": "^6.1.1",
    "@storybook/addon-storyshots": "^6.1.6",
    "@storybook/node-logger": "^6.1.1",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.1",
    "@storybook/theming": "^6.1.10",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.10",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.0",
    "ajv": "^6.9.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^5.2.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "jest": "^24.9.0",
    "jest-junit": "^12.0.0",
    "nan": "^2.14.1",
    "node-forge": "0.10.0",
    "prettier": "2.0.5",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^2.6.2",
    "source-map-explorer": "^2.5.0",
    "typescript": "^3.7.5"
  },
  "proxy": "http://localhost:8080/"
}
