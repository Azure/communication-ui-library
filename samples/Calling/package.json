{
  "name": "calling",
  "version": "0.0.1",
  "private": true,
  "description": "This is a sample application to show how the UI Library can be used to build a calling experience. The client-side application is a React based user interface which uses React Hooks for handling complex state while leveraging Microsoft Fluent UI for how things look.",
  "scripts": {
    "build": "rushx clean && concurrently --kill-others-on-fail \"tsc\" \"webpack --mode=production --env production\"",
    "clean": "rimraf dist",
    "package": "copyfiles -E -u 3 \"../Server/dist/**/*\" dist",
    "start": "concurrently \"cd ../Server && rushx start:dev\" \"rushx start:app\"",
    "start:app": "webpack-cli serve --mode=development --env development",
    "start:prod": "node dist/server.js",
    "test": "",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ../../.prettierrc --ignore-path=../../.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "lint": "eslint */**/*.{ts,tsx}",
    "lint:fix": "eslint */**/*.{ts,tsx} --fix",
    "lint:quiet": "eslint */**/*.{ts,tsx} --quiet"
  },
  "dependencies": {
    "@azure/communication-identity": "1.0.0",
    "@azure/communication-calling": "file:../../azure-communication-calling-1.3.1-beta-rc.1.3.tar",
    "@azure/communication-common": "1.0.0",
    "@azure/communication-react": "1.0.0-beta.7",
    "@azure/core-http": "1.2.4",
    "@babel/preset-react": "^7.12.7",
    "@fluentui/react": "~8.17.2",
    "@fluentui/react-icons": "~1.1.137",
    "@internal/acs-ui-common": "1.0.0-beta.7",
    "@internal/calling-component-bindings": "1.0.0-beta.7",
    "@internal/calling-stateful-client": "1.0.0-beta.7",
    "@internal/react-components": "1.0.0-beta.7",
    "@internal/react-composites": "1.0.0-beta.7",
    "jquery": "^3.5.1",
    "merge": "^2.1.1",
    "mobile-detect": "~1.4.5",
    "react": "~16.14.0",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.4.1",
    "reselect": "~4.0.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "~26.0.22",
    "@types/node": "^14.14.10",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ajv": "^6.9.1",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "8.1.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "~7.0.3",
    "css-loader": "~4.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "html-webpack-plugin": "~5.3.1",
    "husky": "^4.3.0",
    "jest": "26.6.0",
    "jest-junit": "^12.0.0",
    "prettier": "2.3.1",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^2.6.2",
    "source-map-explorer": "^2.5.0",
    "style-loader": "~2.0.0",
    "ts-loader": "^8.0.12",
    "typescript": "4.3.5",
    "url-loader": "~4.1.1",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  }
}
