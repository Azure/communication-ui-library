{
  "name": "acs-server-sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "webpack",
    "start": "ts-node ./bin/www",
    "start:dev": "ts-node-dev ./bin/www",
    "start:debug": "node --inspect=9229 -r ts-node/register ./bin/www",
    "test": "jest",
    "prettier": "prettier --write --config .prettierrc src/**/*",
    "prettier:check": "prettier --check --config .prettierrc src/**/*",
    "lint": "eslint src src --ext .ts",
    "lint:fix": "eslint src src --ext .ts --fix",
    "lint:quiet": "eslint src src --ext .ts --quiet"
  },
  "dependencies": {
    "@azure/communication-administration": "^1.0.0-beta.2",
    "@azure/communication-chat": "^1.0.0-beta.3",
    "@azure/communication-common": "^1.0.0-beta.3",
    "@types/cors": "^2.8.8",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/express": "^4.17.8",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.20",
    "@types/supertest": "^2.0.10",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "copy-webpack-plugin": "^6.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "2.0.5",
    "pretty-quick": "^3.1.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "webpack": "4.42.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2",
